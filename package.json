{
  "name": "booky.io",
  "version": "2.0.0",
  "description": "Online bookmarking service with high focus on performance, mobile support and customizability.",
  "homepage": "https://booky.io",
  "email": "hello@booky.io",
  "private": true,
  "jsnext:main": "_source/index.js",
  "engines": {
    "npm": ">=4"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start": "concurrently -k -r -s first \"npm run open:source\"",
    "open:source": "babel-node tools/srcServer.js",
    "open:public": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* _source tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir _public",
    "remove-dist": "rimraf ./_public",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "babel-node tools/build.js && npm run open:public",
    "build:quick": "babel-node tools/build.js",
    "test": "jest --config jest.config.json --no-cache",
    "test:watch": "npm run test -- --watch",
    "test:cover": "jest --config jest.config.json --no-cache --coverage",
    "qa": "npm-run-all test lint",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "storybook": "start-storybook -p 666 -c storybook -s ./_public"
  },
  "proxy": "http://localhost:8001/",
  "author": "Nico Thiebes",
  "license": "SEE LICENSE IN \"LICENSE\"",
  "dependencies": {
    "classnames": "2.2.5",
    "es-cookie": "1.2.0",
    "object-assign": "4.1.1",
    "react": "16.3.0",
    "react-beautiful-dnd": "6.0.1",
    "react-dom": "16.3.0",
    "react-intl": "2.4.0",
    "react-intl-redux": "2.0.2",
    "react-redux": "5.0.7",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@storybook/addon-viewport": "3.4.10",
    "@storybook/react": "3.4.10",
    "autoprefixer": "7.1.4",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.6",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.12",
    "chalk": "2.0.1",
    "concurrently": "3.5.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.5.1",
    "coveralls": "2.13.1",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.0.0",
    "eslint": "4.2.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "3.1.2",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "0.11.2",
    "history": "4.6.0",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "jest-cli": "22.4.3",
    "json-loader": "0.5.4",
    "node-sass": "4.5.3",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.6",
    "prompt": "1.0.0",
    "prop-types": "15.6.1",
    "raf": "3.3.2",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-storybook-addon-chapters": "2.1.5",
    "react-test-renderer": "16.0.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.3.0",
    "replace": "0.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.9",
    "webpack": "3.1.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/nthiebes/booky.io.git"
  }
}
