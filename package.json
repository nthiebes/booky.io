{
  "name": "booky.io",
  "version": "2.0.1",
  "extensionVersion": "2.0.5",
  "description": "Online bookmarking service with high focus on performance, mobile support and customizability.",
  "homepage": "https://booky.io",
  "email": "hello@booky.io",
  "private": true,
  "jsnext:main": "_source/index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=4"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start": "npm run open:source",
    "open:source": "babel-node tools/srcServer.js",
    "open:public": "babel-node tools/distServer.js",
    "lint": "npm-run-all lint:css lint:js",
    "lint:js": "eslint .",
    "lint:css": "stylelint _source/**/*.scss",
    "build:open": "babel-node tools/build.js && npm run open:public",
    "build": "babel-node tools/build.js",
    "test": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:watch": "jest --watchAll",
    "qa": "npm-run-all test lint",
    "storybook": "start-storybook -p 666 -c storybook -s ./_public",
    "extension": "cd extension && ./manifest.sh firefox $npm_package_extensionVersion && ./manifest.sh opera $npm_package_extensionVersion && ./manifest.sh edge $npm_package_extensionVersion && ./manifest.sh chrome $npm_package_extensionVersion",
    "extension:chrome": "cd extension && ./manifest.sh chrome $npm_package_extensionVersion",
    "extension:firefox": "cd extension && ./manifest.sh firefox $npm_package_extensionVersion",
    "extension:opera": "cd extension && ./manifest.sh opera $npm_package_extensionVersion",
    "extension:edge": "cd extension && ./manifest.sh edge $npm_package_extensionVersion"
  },
  "proxy": "http://localhost:8001/",
  "author": "Nico Thiebes",
  "license": "SEE LICENSE IN \"LICENSE\"",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@hot-loader/react-dom": "16.13.0",
    "@sentry/browser": "5.23.0",
    "abortcontroller-polyfill": "1.5.0",
    "classnames": "2.2.6",
    "connected-react-router": "6.8.0",
    "core-js": "3.6.5",
    "date-fns": "2.16.1",
    "disqus-react": "1.0.10",
    "es-cookie": "1.3.2",
    "intersection-observer": "0.11.0",
    "npm-run-all": "4.1.5",
    "object-assign": "4.1.1",
    "react": "16.13.1",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "16.13.1",
    "react-dropzone": "11.2.0",
    "react-intersection-observer": "8.29.0",
    "react-intl": "5.8.1",
    "react-intl-redux": "2.2.0",
    "react-redux": "7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "whatwg-fetch": "3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-transform-react-constant-elements": "7.10.4",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@storybook/addon-viewport": "5.3.21",
    "@storybook/react": "5.3.21",
    "acorn": "8.0.1",
    "ajv": "6.12.5",
    "autoprefixer": "9.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.12",
    "chalk": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "connect-history-api-fallback": "1.6.0",
    "copy-webpack-plugin": "6.1.0",
    "coveralls": "3.1.0",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "eslint": "7.9.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-watch": "7.0.0",
    "file-loader": "6.1.0",
    "hard-source-webpack-plugin": "0.13.1",
    "history": "4.10.1",
    "html-webpack-plugin": "4.4.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.4.2",
    "jest-cli": "26.4.2",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.11.2",
    "mockdate": "3.0.2",
    "node-sass": "4.14.1",
    "opn-cli": "4.1.0",
    "postcss": "7.0.32",
    "postcss-loader": "4.0.1",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-hot-loader": "4.12.21",
    "react-storybook-addon-chapters": "3.1.7",
    "react-test-renderer": "16.13.1",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.4",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "style-loader": "1.2.1",
    "stylelint": "13.7.1",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-recommended": "3.0.0",
    "url-loader": "4.1.0",
    "stylelint-config-standard": "20.0.0",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/nthiebes/booky.io.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  }
}
