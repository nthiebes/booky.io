{
  "name": "booky.io",
  "version": "2.0.0",
  "description": "Online bookmarking service with high focus on performance, mobile support and customizability.",
  "homepage": "https://booky.io",
  "email": "hello@booky.io",
  "private": true,
  "jsnext:main": "_source/index.js",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start": "npm-run-all --parallel test:watch open:source",
    "start:quick": "npm run open:source",
    "open:source": "babel-node tools/srcServer.js",
    "open:public": "babel-node tools/distServer.js",
    "lint": "./node_modules/.bin/esw \"_source/**/*.{js,jsx}\" --ext .js,.jsx --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir _public",
    "remove-dist": "rimraf ./_public",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:public",
    "build:quick": "babel-node tools/build.js",
    "test": "jest --config jest.config.json --no-cache",
    "test:watch": "npm run test -- --watch",
    "test:cover": "jest --config jest.config.json --no-cache --coverage",
    "qa": "npm-run-all test lint"
  },
  "author": "Nico Thiebes",
  "license": "SEE LICENSE IN \"LICENSE\"",
  "dependencies": {
    "classnames": "2.2.5",
    "object-assign": "4.1.1",
    "prop-types": "15.6.1",
    "react": "16.3.0",
    "react-beautiful-dnd": "6.0.1",
    "react-dom": "16.3.0",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.12",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.22.0",
    "babel-register": "6.22.0",
    "browser-sync": "2.18.7",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.4",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "eslint": "4.11.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.22.0",
    "jest": "21.2.1",
    "jsdoc": "3.4.1",
    "jsdoc-to-markdown": "1.3.7",
    "minami": "1.1.1",
    "mockdate": "1.0.4",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.1",
    "open": "0.0.5",
    "postcss-loader": "1.2.2",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.2",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.10.0",
    "webpack-hot-middleware": "2.16.1",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  }
}
